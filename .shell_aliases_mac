PATH=~/.rbenv/shims:$HOME/bin:/usr/local/sbin:/usr/local/bin:~/Library/Python/2.7/bin:$PATH

export LANG="en_US.UTF-8"
export LC_ALL="en_US.UTF-8"
export LESSCHARSET=utf-8

export LS_OPTIONS='--color=auto'
export CLICOLOR='Yes'
export LSCOLORS=''

alias ll="ls -la"

alias hide="setfile -a V"
alias show="setfile -a v"

alias flushdns="sudo killall -HUP mDNSResponder;sudo discoveryutil mdnsflushcache;sudo discoveryutil udnsflushcaches"
alias pythonserver="python -m SimpleHTTPServer"

eval "$(rbenv init -)"

alias proxy="export https_proxy=http://127.0.0.1:6152;export http_proxy=http://127.0.0.1:6152"
